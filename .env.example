# Eve Online SSO Configuration
# Get your Client ID and Secret from: https://developers.eveonline.com/applications
EVE_CLIENT_ID=your_client_id_here
EVE_CLIENT_SECRET=your_client_secret_here
EVE_CALLBACK_URL=http://localhost:3000/api/auth/callback

# Server Configuration
PORT=3000
NODE_ENV=development
SESSION_SECRET=your_random_session_secret_here

# Frontend URL
CLIENT_URL=http://localhost:3001

# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_ADMIN_USER_ID=your_discord_user_id_here

# SDE (Static Data Export) Configuration
SDE_DATA_DIR=eve-online-static-data
SDE_CHECK_INTERVAL=86400000
