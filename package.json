{
  "name": "whatdidimine",
  "version": "1.0.0",
  "description": "Eve Online mining tracker with multi-character support",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cross-env NODE_ENV=development nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:prod": "cross-env NODE_ENV=production node server/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/index.js",
    "install:all": "npm install && cd client && npm install",
    "import:sde": "node server/scripts/import-sde.js",
    "migrate:timestamps": "node server/database/migrations/add-timestamps.js",
    "sde:check": "node server/scripts/sde-check.js",
    "sde:update": "node server/scripts/sde-update.js",
    "sde:version": "node server/scripts/sde-version.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "keywords": [
    "eve-online",
    "mining",
    "esi",
    "sso"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "adm-zip": "^0.5.16",
    "axios": "^1.6.2",
    "better-sqlite3": "^12.5.0",
    "better-sqlite3-session-store": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "winston": "^3.19.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  }
}
